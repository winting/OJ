class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int len=prices.size();
        if(len<2)
            return 0;
        int low = prices[0];
        int max=0;
        for(int i=1;i<len;i++)
        {
            if(prices[i]-low>max)
                max=prices[i]-low;
            if(prices[i]<low)
                low=prices[i];
        }
        return max;
    }
};
